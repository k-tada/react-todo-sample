{
  "name": "react_template",
  "version": "0.0.1",
  "description": "React + Webpack + Bable Template",
  "main": "index.js",
  "scripts": {
    "clean:www": "rm -rf dist",
    "clean": "npm run clean:www",
    "copy:html": "cp src/index.html www/",
    "copy": "npm run copy:html",
    "build": "webpack",
    "watch": "npm run clean && npm run copy && webpack --watch & `npm bin`/browser-sync start --config bs-config.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "~15.3.2",
    "react-addons-update": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-css-modules": "^3.7.6",
    "react-dom": "~15.3.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "browser-sync": "^2.13.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "^1.0.1",
    "webpack": "~1.13.3"
  }
}
